{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "composite": true,
    "outDir": "dist",
    "baseUrl": "."
  },
  "include": ["src"]
}
